{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "error",
        "semi": "error",
        "no-unused-vars": "error",
        "no-undef": "error"
    }
  }